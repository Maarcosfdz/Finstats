CREATE TABLE category (
    nombre TEXT NOT NULL,
    identgGasto INTEGER NOT NULL,
    CONSTRAINT PKcategory PRIMARY KEY (nombre),
    CONSTRAINT UQ_category_nombre UNIQUE (nombre)
);

CREATE TABLE movimiento (
    id INTEGER PRIMARY KEY,  -- Auto-incrementa automáticamente en SQLite
    nombre TEXT NOT NULL,
    cant REAL NOT NULL,  -- FLOAT reemplazado por REAL
    data DATE NOT NULL,
    categoryName TEXT NOT NULL,
    identGasto INTEGER NOT NULL,
    CONSTRAINT categoryNameFK FOREIGN KEY (categoryName)
        REFERENCES category(nombre) ON DELETE CASCADE
);
